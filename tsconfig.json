{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "baseUrl": "."
  },
  "include": ["security/src", "scripts"],
  "exclude": ["supabase/**", "caregiver-dashboard/**", "lib/**", "android/**", "ios/**"]
}
